{
  "name": "@waseful/sdk",
  "private": true,
  "version": "0.0.0",
  "source": "src/sdk.ts",
  "lib": "dist/lib.js",
  "web": "dist/sdk.js",
  "targets": {
    "web": {
      "outputFormat": "global",
      "distDir": "./dist"
    },
    "lib": {
      "outputFormat": "commonjs",
      "distDir": "./dist"
    }
  },
  "alias": {
    "preact/jsx-dev-runtime": "preact/jsx-runtime"
  },
  "scripts": {
    "serve": "parcel ./index.html --https -p 8081",
    "dev": "npm run serve",
    "build": "parcel build",
    "lint": "eslint '{src,tests}/**/*.{ts,tsx}'",
    "test": "vitest run",
    "tsc": "tsc"
  },
  "dependencies": {
    "preact": "^10.13.1",
    "preact-habitat": "^3.3.0",
    "unfetch": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/preact": "^3.2.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/uuid": "^9.0.1",
    "jsdom": "^20.0.3",
    "parcel": "^2.8.3",
    "vitest": "^0.30.1"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "preact",
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "build/"
    ]
  }
}
